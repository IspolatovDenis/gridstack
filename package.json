{
  "name": "@trysound/gridstack",
  "version": "0.6.0",
  "description": "gridstack is a jQuery plugin for widget layout",
  "main": "dist/gridstack.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trysound/gridstack.git"
  },
  "scripts": {
    "build": "grunt && webpack",
    "prepublish": "yarn run build",
    "test": "cross-env BABEL_ENV=test jest --coverage",
    "posttest": "eslint src"
  },
  "jest": {
    "coverageReporters": [
      "text-summary",
      "html"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "keywords": [
    "gridstack",
    "grid",
    "gridster",
    "layout",
    "jquery"
  ],
  "author": "Pavel Reznikov <pashka.reznikov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trysound/gridstack/issues"
  },
  "homepage": "http://trysound.github.io/gridstack",
  "dependencies": {
    "jquery": "^3.1.0",
    "jquery-ui": "^1.12.0",
    "lodash": "^4.14.2"
  },
  "devDependencies": {
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "cross-env": "^3.2.4",
    "doctoc": "^1.0.0",
    "eslint": "^3.18.0",
    "grunt": "^0.4.5",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-cssmin": "^0.14.0",
    "grunt-doctoc": "^0.1.1",
    "grunt-sass": "^1.1.0",
    "jest": "^19.0.2",
    "webpack": "^2.3.2"
  }
}
